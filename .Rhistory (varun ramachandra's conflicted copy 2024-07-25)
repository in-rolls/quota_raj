ps_descriptive_stats <- raj_panch %>%
group_by(ps_2020) %>%
summarise(
mean_treat_2015 = mean(treat_2015, na.rm = TRUE),
mean_treat_2020 = mean(treat_2020, na.rm = TRUE),
mean_sex_2020 = mean(sex_2020 == "F", na.rm = TRUE),
mean_always_treated = mean(always_treated, na.rm = TRUE),
) %>%
ungroup()
View(ps_descriptive_stats)
ps_descriptive_stats <- raj_panch %>%
group_by(ps_2020) %>%
summarise(
mean_treat_2010 = mean(treat_2010, na.rm = TRUE),
mean_treat_2015 = mean(treat_2015, na.rm = TRUE),
mean_treat_2020 = mean(treat_2020, na.rm = TRUE),
mean_sex_2020 = mean(sex_2020 == "F", na.rm = TRUE),
mean_always_treated = mean(always_treated, na.rm = TRUE),
) %>%
ungroup()
ps_descriptive_stats <- raj_panch %>%
group_by(ps_2020) %>%
summarise(
mean_treat_2010 = mean(treat_2010, na.rm = TRUE),
mean_treat_2015 = mean(treat_2015, na.rm = TRUE),
mean_treat_2020 = mean(treat_2020, na.rm = TRUE),
mean_sex_2010 = mean(sex_2010 == "F", na.rm = TRUE),
mean_sex_2015 = mean(sex_manual_2015 == "F", na.rm = TRUE),
mean_sex_2020 = mean(sex_2020 == "F", na.rm = TRUE),
mean_always_treated = mean(always_treated, na.rm = TRUE),
) %>%
ungroup()
View(ps_descriptive_stats)
raj_panch <- raj_panch %>%
group_by(ps_2020) %>%
mutate(info_spillover_2015 = if_else(any(treat_2015), 1, 0)) %>%
ungroup()
m_info_spillover <- feols((sex_2020 == "F") ~ treat_2015 + info_spillover_2015 | district_2020 + ps_2020,
data = filter(raj_panch, treat_2020 == 0))
summary(m_info_spillover)
View(raj_panch)
unique(raj_panch$info_spillover_2015)
raj_panch <- raj_panch %>%
group_by(district_2020) %>%
mutate(info_spillover_2015 = if_else(any(treat_2015), 1, 0)) %>%
ungroup()
View(raj_panch)
unique(raj_panch$info_spillover_2015)
